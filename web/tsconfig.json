{
    "compilerOptions": {
        "esModuleInterop": true,
        "jsx": "react",
        "module": "ESNext",
        "moduleResolution": "node",
        "lib": [
            "dom",
            "ESNext"
        ],
        "strict": true,
        "sourceMap": true,
        "target": "ESNext",
        "baseUrl": ".",
        "paths": {
            "@components/*" : ["./src/components/*"]
        }
    },
    "exclude": [
        "node_modules"
    ]
}